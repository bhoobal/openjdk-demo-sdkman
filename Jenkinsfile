pipeline {
    agent any
        
    tools {
        jdk "JDK-17"
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
         
            }
        }
      
        stage('gradle') {
            steps {
                echo 'gradle....'
                sh 'java -version'
            }
        }
    }
}
